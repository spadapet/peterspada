{
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "dest": "../docs",
    "fileMetadataFiles": [],
    "globalMetadata": {
      "_appLogoPath": "images/logo.svg",
      "_disableBreadcrumb": "true",
      "_disableContribution": "true",
      "_appTitle": "Peter Spada"
    },
    "globalMetadataFiles": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "postProcessors": [],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "template": [
      "default"
    ]
  }
}